  resolution varchar(20),
  frame_rate double,
  codec varchar(50),
  languages varchar(100),
  subtitle_languages varchar(100),
  dubbed_languages varchar(100),
  bit_rate bigint,
  content_filesize bigint,
  content_checksum varchar(50),
  trickmodesrestricted varchar(100),
